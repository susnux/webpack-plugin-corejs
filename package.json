{
	"name": "webpack-plugin-corejs",
	"version": "0.2.1",
	"description": "âš¡ Include core-js polyfills when bundling with webpack",
	"keywords": [
		"corejs",
		"core-js",
		"polyfill",
		"polyfills",
		"webpack",
		"webpack4",
		"webpack5",
		"webpackplugin"
	],
	"license": "EUPL-1.2",
	"repository": "susnux/webpack-plugin-corejs",
	"files": [
		"dist",
		"LICENSES",
		"CHANGELOG.md"
	],
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/cjs/index.js"
	},
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsc && tsc --project tsconfig.cjs.json",
		"format": "prettier --write src '*.{js,mjs,cjs}'",
		"lint": "prettier --check src '*.{js,mjs,cjs}'",
		"test": "jest",
		"prepack": "npm run build"
	},
	"engines": {
		"node": "^16.0.0"
	},
	"peerDependencies": {
		"webpack": "^4.44.0 || ^5.0"
	},
	"dependencies": {
		"core-js-builder": "^3.38.0"
	},
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@types/node": "^16.18.28",
		"@types/webpack": "^4.41.38",
		"@types/webpack-sources": "^3.2.3",
		"browserslist": "^4.21.5",
		"jest": "^29.7.0",
		"prettier": "^2.8.8",
		"ts-jest": "^29.2.4",
		"typescript": "^5.0.4",
		"webpack": "^4.47.0",
		"webpack-test-utils": "^2.1.0",
		"webpack5": "npm:webpack@^5.82.1"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"semi": false
	}
